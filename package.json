{
  "name": "isometric-bar-chart",
  "version": "0.0.11",
  "description": "isometric bar chart",
  "keywords": [
    "chart",
    "isometric",
    "barchart"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "src/**/*",
    "dist/**/*"
  ],
  "scripts": {
